		]
	}, {
		"module": "node",
		"stat": "socket_ops",
		"fields": [
	    	    "hostname", "zonename", "pid", "execname", "psargs", "ppid",
	    	    "pexecname", "ppsargs", "raddr", "rport", "optype", "size",
	    	    "buffered"
		]
	}, {
		"module": "syscall",
		"stat": "syscalls",
