		]
	}, {
		"module": "node",
		"stat": "gc_ops",
		"fields": [
		    "hostname", "zonename", "pid", "execname", "psargs", "ppid",
		    "pexecname", "ppsargs", "gctype", "latency"
		]
	}, {
		"module": "node",
		"stat": "httpc_ops",
		"fields": [
		    "hostname", "zonename", "pid", "execname", "psargs", "ppid",
		    "pexecname", "ppsargs", "raddr", "rport", "http_method",
		    "http_url", "http_path", "latency"
		]
	}, {
		"module": "node",
		"stat": "httpd_ops",
		"fields": [
		    "hostname", "zonename", "pid", "execname", "psargs", "ppid",
