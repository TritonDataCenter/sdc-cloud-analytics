		"fields": [ "hostname", "cpu", "utilization" ]
	}, {
		"module": "cpu",
		"stat": "thread_executions",
		"fields": [
		    "hostname", "zonename", "pid", "execname", "psargs",
		    "ppid", "pexecname", "ppsargs", "leavereason", "runtime"
		]
	}, {
